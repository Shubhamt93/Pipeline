pipeline  {
    agent any
    stages {
        stage ('Pull-code') {
            steps {
                sh print
            }
        }
        stage ('Print-Env')  {
             steps  {
                 echo "Build Id is ${env.BUILD_ID}"
                 echo "Node Label is ${env.NODE_LABELS}"
                 echo "Git Commit is ${env.GIT_COMMIT}"
                 echo "Git Branch is ${env.GIT_BRANCH}"
             }   
        }
    }
}