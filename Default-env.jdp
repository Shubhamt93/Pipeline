pipeline  {
    agent any
    stages {
        stage ('Pull-Code')  {
            steps {
                sh 'printenv'
            }
        }
        stage ('Print-Env')  {
             steps  {
                 echo "Build Id is ${env.BUILD_ID}"
                 echo "Node Label is ${env.NODE_LABELS}"
                 echo "Git Commit is ${GIT_COMMIT}"
                 echo "Git Branch is ${GIT_BRANCH}"
             }   
        }
    }
}